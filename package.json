{
  "name": "boilerplate-react-typescript-vite-firebase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host --config vite-host.config.ts",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install",
    "preview": "vite preview",
    "gac": "git add . && git commit -m",
    "gac:nv": "git add . && git commit --no-verify -m",
    "gp": "git pull && yarn install",
    "sync": "yarn gac:nv 'sync' && git push"
  },
  "dependencies": {
    "firebase": "^10.5.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.18.0",
    "react-toastify": "^9.1.3",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-linter": "^2.0.7",
    "vite-plugin-svgr": "^4.1.0",
    "vite-plugin-terminal": "^1.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
